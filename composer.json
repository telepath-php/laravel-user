{
    "name": "telepath/laravel-user",
    "description": "Provides a Telegram User authentication driver.",
    "type": "library",
    "require": {
        "php": "^8.1",
        "illuminate/support": "^9.0 | ^10.0",
        "telepath/laravel": "^0.4.0"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Telepath\\Laravel\\TelegramUser\\": "src/",
            "Telepath\\Laravel\\TelegramUser\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Tii",
            "email": "mail@tii.one"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "orchestra/testbench": "^8.0",
        "pestphp/pest": "^2.3",
        "pestphp/pest-plugin-laravel": "^2.0",
        "pestphp/pest-plugin-faker": "^2.0"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
